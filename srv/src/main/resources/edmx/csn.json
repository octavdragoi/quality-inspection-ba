      "kind": "entity",
      "@Catalog.tableType": {
        "#": "COLUMN"
      },
      "@cds.persistence.name": "VIEWCONSUMERSERVICES_COUNTDEFECT",
      "@readOnly": true,
      "query": {
        "SELECT": {
          "from": {
            "ref": [
              "quality.Products_Defects"
            ]
          },
          "columns": [
            {
              "func": "count",
              "args": [
                {
                  "ref": [
                    "product",
                    "ID"
                  ]
                }
              ],
              "as": "NumDefect",
              "cast": {
                "type": "cds.Integer"
              }
            },
            {
              "key": true,
              "ref": [
                "defect",
                "description"
              ],
              "as": "defect"
            }
          ],
          "groupBy": [
            {
              "ref": [
                "defect",
                "description"
              ]
            }
          ],
          "where": [
            "not",
            "(",
            {
              "ref": [
                "defect",
                "ID"
              ]
            },
            "=",
            {
              "val": 6
            },
            ")"
          ]
        }
      },
      "elements": {
        "NumDefect": {
          "@Core.Computed": true,
          "@cds.persistence.name": "NUMDEFECT",
          "type": "cds.Integer"
